@prefix ex: <http://example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ex:Person a owl:Class ;
  rdfs:label "Person" , "Personn" .

ex:name a owl:DatatypeProperty ;
  skos:prefLabel "name" , "namee" .

ex:hasFriend a owl:ObjectProperty ;
  rdfs:label "hasFriend"@en , "hasFriendd"@en ;
  skos:prefLabel "hasFriend"@en , "hasFriendd"@en .

ex:note a owl:AnnotationProperty ;
  skos:prefLabel "note" , "notee" , "note"@en , "notee"@en ;
  rdfs:label "note" , "notee" , "note"@en , "notee"@en .
